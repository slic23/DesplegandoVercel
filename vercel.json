{
    "builds": [
        {
            "src": "locallibrary/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "migrate_modelDB.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        },
        {
            "src": "start.sh",
            "use": "@vercel/static-build",
            "config": {
                "command": "bash start.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "locallibrary/wsgi.py"
        }
    ]
}
